{
  "name": "ts-jackson",
  "version": "1.4.0",
  "author": "Ilias Gazdaliev",
  "description": "",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "postbuild": "npm run tsc-release && npm run pretify-dist",
    "tsc-release": "tsc --build ts-config.release.json",
    "pretify-dist": "cd ./dist && prettier --write .",
    "test": "jest",
    "watch": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Eljoy/ts-jackson.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Eljoy/ts-jackson/issues"
  },
  "keywords": [
    "json",
    "typescript",
    "serializer",
    "deserializer",
    "decorator",
    "metadata",
    "serialize",
    "deserialize",
    "json serialize",
    "json deserialize",
    "DTO",
    "Data Transfer Objects",
    "Jackson",
    "Object mapper",
    "json-mapper",
    "json-adapter",
    "json-transformer",
    "typescript-json",
    "api-adapter",
    "api-mapper"
  ],
  "homepage": "https://github.com/Eljoy/ts-jackson#readme",
  "dependencies": {
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-typescript": "^8.2.0",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "eslint": "^7.25.0",
    "eslint-plugin-react": "^7.23.2",
    "husky": "^4.3.7",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "prettier-plugin-organize-imports": "^1.1.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.41.5",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^26.5.4",
    "tslib": "^2.2.0",
    "typescript": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --allow-empty",
      "pre-push": "tsc && jest"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  }
}
